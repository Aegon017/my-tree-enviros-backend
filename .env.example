APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=my_tree_enviros
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# SMS Login Configuration
SMS_LOGIN_ENDPOINT=https://smslogin.co/v3/api.php
SMS_LOGIN_USERNAME=MYTREE
SMS_LOGIN_APIKEY=
SMS_LOGIN_SENDERID=MYTREN
SMS_LOGIN_OTP_TEMPLATE_ID=

# Razorpay Configuration
RAZORPAY_KEY=
RAZORPAY_SECRET=
RAZORPAY_WEBHOOK_SECRET=

# Firebase/FCM Configuration
# Path to your Firebase service account JSON file
# Example: /var/www/storage/app/firebase/credentials.json
# Or on Windows: C:\path\to\firebase-credentials.json
FIREBASE_CREDENTIALS=

# Firebase Database URL (optional)
FIREBASE_DATABASE_URL=

# Firebase Storage Bucket (optional)
FIREBASE_STORAGE_BUCKET=

# Firebase Project ID
FIREBASE_PROJECT_ID=

# FCM Server Key (legacy, optional)
FCM_SERVER_KEY=

# FCM Default Settings
FCM_DEFAULT_SOUND=default
FCM_DEFAULT_BADGE=1
FCM_PRIORITY=high
FCM_TTL=3600

# Firebase Logging (optional)
FIREBASE_LOGGING_ENABLED=false
FIREBASE_LOGGING_CHANNEL=stack

# Firebase HTTP Client Options
FIREBASE_HTTP_TIMEOUT=30
FIREBASE_CACHE_STORE=file
FIREBASE_VERIFY=true
